
cmake_minimum_required(VERSION 2.8)

PROJECT(ApplyingVtkMarchingCubes)

find_package(VTK REQUIRED)
include(${VTK_USE_FILE})

add_executable(ApplyingVtkMarchingCubes ApplyingVtkMarchingCubes.cxx)

if(VTK_LIBRARIES)
  target_link_libraries(PolyDataToImageData ${VTK_LIBRARIES})
else()
  target_link_libraries(PolyDataToImageData vtkHybrid)
endif()
